<div class="tracks-container">
    {{#each data.body.items}}
    <div class="album-tracks">
        <h3>{{this.name}}</h3>

        <audio>
            <source src="{{this.preview_url}}" type="audio/MP3">
        </audio>
        <audio controls>
            <source src="{{this.preview_url}}" type="audio/mpeg">
            Your browser does not support the audio tag.
        </audio>
    </div>
    {{/each}}
</div>